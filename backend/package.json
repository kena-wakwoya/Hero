  "scripts": {
   "build": "rm -rf build/* && babel -d ./build ./src",
   "start": "node ./build/server.js",
   "dev": "rm -rf build/* && nodemon --exec babel-node -- ./src/server.js",
   "prettier": "prettier --write 'src/**/*.{js}'",
   "lint": "eslint ./src",
