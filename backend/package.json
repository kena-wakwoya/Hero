   "@babel/plugin-proposal-class-properties": "^7.18.6",
   "@babel/plugin-transform-runtime": "^7.19.6",
   "@babel/polyfill": "^7.12.1",
   "@babel/preset-env": "^7.19.4",
   "@babel/runtime": "^7.20.0",
   "@types/randomatic": "^3.1.3",
   "babel-eslint": "^10.1.0",
   "eslint": "^7.32.0",
