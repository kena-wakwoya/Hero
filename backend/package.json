   "nodemon": "^2.0.20",
   "prettier": "^2.7.1",
   "prettierrc": "^0.0.0-5",
   "supertest": "^6.3.1"
  },
  "husky": {
   "hooks": {
    "pre-commit": "lint-staged"
   }
  },
  "lint-staged": {
   "src/**/*.{js}": [
    "prettier --write",
    "eslint --fix src/"
   ]
  }
